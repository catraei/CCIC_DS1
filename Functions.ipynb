   "source": [
    "def sort(list):\n",
    "    r = len(list)\n",
    "    for i in range(r-1):\n",
    "        for j in range(r-i-1):\n",
    "            if list[j] > list[j+1]:\n",
    "                list[j], list[j+1] = list[j+1], list[j]\n",
    "    return(list)[::-1]\n",
    "print(sort([1,2,4,7,3]))"
   ]
